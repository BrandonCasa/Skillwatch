{
  "extends": ["eslint:recommended", "react-app", "react-app/jest"],
  "env": {
    "es6": true,
    "node": true
  },
  "rules": {
    "no-prototype-builtins": 0,
    "max-len": ["error", { "code": 240 }],
    "indent": ["error", 2]
  },
  "parserOptions": {
    "sourceType": "module",
    "ecmaFeatures": {
      "jsx": true
    }
  }
}
