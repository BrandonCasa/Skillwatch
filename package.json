{
  "name": "skillwatch",
  "version": "0.0.11",
  "private": true,
  "main": "public/electron.js",
  "homepage": "./",
  "dependencies": {
    "@material-ui/core": "^4.11.3",
    "@material-ui/icons": "^4.11.2",
    "@rematch/core": "^2.0.0-next.10",
    "@testing-library/jest-dom": "^5.11.4",
    "@testing-library/react": "^11.1.0",
    "@testing-library/user-event": "^12.1.10",
    "clsx": "^1.1.1",
    "concurrently": "^6.0.0",
    "electron-drag": "^2.0.0",
    "electron-firebase": "^1.0.0",
    "electron-is-dev": "^2.0.0",
    "electron-log": "^4.3.2",
    "electron-updater": "^4.3.8",
    "express": "^4.17.1",
    "firebase": "^8.3.0",
    "firebase-admin": "^9.6.0",
    "firebase-functions": "^3.13.2",
    "is-electron": "^2.2.0",
    "node-sass": "4.14.1",
    "random-username-generator": "^1.0.4",
    "react": "^17.0.1",
    "react-color": "^2.19.3",
    "react-dom": "^17.0.1",
    "react-redux": "^7.2.2",
    "react-router-dom": "^5.2.0",
    "react-scripts": "4.0.3",
    "redux": "^4.0.5",
    "wait-on": "^5.3.0",
    "web-vitals": "^1.0.1"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject",
    "electron-dev": "electron .",
    "pack": "electron-builder --dir",
    "dist": "electron-builder",
    "distPub": "electron-builder --publish always"
  },
  "build": {
    "productName": "Skillchat",
    "copyright": "Copyright Â© 2021 Brandon Casamichana",
    "appId": "kannatronics.skillchat",
    "files": [
      "build/**/*",
      "node_modules/**/*"
    ],
    "directories": {
      "buildResources": "public"
    },
    "publish": [
      {
        "provider": "github",
        "owner": "GummyEater",
        "repo": "Skillwatch",
        "releaseType": "release"
      }
    ],
    "win": {
      "publish": [
        "github"
      ],
      "target": [
        {
          "target": "nsis",
          "arch": [
            "x64"
          ]
        }
      ]
    }
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "electron": "^12.0.2",
    "electron-builder": "^22.10.5",
    "eslint": "^7.23.0",
    "firebase-functions-test": "^0.2.3",
    "prettier": "^2.2.1"
  }
}
